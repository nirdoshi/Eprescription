<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.napps.voiceprescription">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name="com.napps.voiceprescription.publicarray"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.napps.voiceprescription.duration"></activity>
        <activity android:name="com.napps.voiceprescription.medstime" />
        <activity android:name="com.napps.voiceprescription.Editinfo" />
        <activity android:name="com.napps.voiceprescription.Exsistinpatient" />
        <activity android:name="com.napps.voiceprescription.prescription" />
        <activity android:name="com.napps.voiceprescription.Symptoms" />
        <activity android:name="com.napps.voiceprescription.Name" />
        <activity android:name="com.napps.voiceprescription.Account" />
        <activity android:name="com.napps.voiceprescription.Registration" />
        <activity
            android:name="com.napps.voiceprescription.Login"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.napps.voiceprescription.MainActivity" />
    </application>

</manifest>